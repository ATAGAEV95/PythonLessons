    "import time\n",
    "import threading\n",
    "\n",
    "\n",
    "def get_data(data):\n",
    "    for _ in range(5):\n",
    "        print(f\"[{threading.current_thread().name}] {data}\")\n",
    "        time.sleep(1)\n",
    "\n",
    "\n",
    "thr = threading.Thread(target=get_data, args=(str(time.time()),))\n",
    "thr.start()\n",
    "\n",
    "print('finish')"
   ],
